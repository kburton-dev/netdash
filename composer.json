{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^11.6.0",
        "laravel/octane": "^2.3.9",
        "laravel/sanctum": "^4.0.2",
        "laravel/tinker": "^2.9.0",
        "livewire/livewire": "^3.4.12",
        "livewire/volt": "^1.6.3",
        "saloonphp/xml-wrangler": "^1.2.1",
        "spatie/flysystem-dropbox": "^3.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "barryvdh/laravel-ide-helper": "^3",
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.9.5",
        "laravel/breeze": "^2.0.3",
        "laravel/pint": "^1.15.3",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^2.34.7",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sa": [
            "@php vendor/bin/phpstan analyse"
        ],
        "gb": [
            "@php vendor/bin/phpstan analyse --generate-baseline"
        ],
        "dm": [
            "@php artisan ide-helper:models -W",
            "vendor/bin/pint app/Models"
        ],
        "qa": [
            "vendor/bin/pint --test",
            "vendor/bin/phpstan analyse",
            "vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
